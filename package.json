{
  "name": "shipbubble",
  "version": "1.0.4",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "description": "Shipbubble API SDK for Node.js",
  "author": "Shalom Ogunshola",
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Shipbubble/node-sdk"
  },
  "keywords": [
    "shipbubble",
    "sdk",
    "api",
    "typescript",
    "openapi",
    "logistics",
    "shipping"
  ],
  "dependencies": {
    "axios": "^1.6.0"
  },
  "scripts": {
    "build": "tsc",
    "gen:api-types": "openapi-typescript ./openapi.yaml --output ./src/types/api.ts",
    "format": "yarn prettier . \"**/*.+(js|ts|json)\" --write",
    "format:check": "yarn prettier . \"**/*.+(js|ts|json)\" --check"
  },
  "devDependencies": {
    "@types/node": "^20.8.9",
    "ts-node": "^10.9.1",
    "eslint": "^8.23.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "28",
    "ts-jest": "^28.0.3",
    "prettier": "^2.7.1",
    "openapi-types": "^12.0.0",
    "openapi-typescript": "^5.2.0",
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "typescript": "^5.2.2"
  }
}
